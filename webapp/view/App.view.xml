<mvc:View
	controllerName="iamsoft.test.component.reuse.controller.App"
	xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
	displayBlock="true"
	xmlns="sap.m">
	<App>
		<pages>
			<Page
				id="page"
				title="{i18n>title}">
				<content>
                    <Input value="{/buttonText}" />
                    <core:ComponentContainer propagateModel="true" usage="myreuse" async="true"></core:ComponentContainer>
                </content>
			</Page>
		</pages>
	</App>
</mvc:View>
